
@{
    ViewBag.Title = "Patient Registrations";
}
<!-- Content Header (Page header) -->
    <section class="content-header">
        <h2>
            Patient Registration
        </h2>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
            <li class="active">Here</li>
        </ol>
    </section>


    <div class="col-md-6">
        <div class="box box-primary">
            
                <div class="text-center">
                    <h3 class="box-title">Patient Information</h3>
                </div>
            
            <div class="box-body">
                <form role="form">
                    <div class="col-xs-6">
                        <label>Patient File Number</label>
                        <input type="text" class="form-control" placeholder="" autocomplete="off">
                    </div>

                    <!-- Enrolled Date -->
                    <div class="form-group">

                        <label>Enrolled Date:</label>

                        <div class="input-group date">
                            <div class="col-xs-5">
                                <i class="fa fa-calendar"></i>
                            </div>
                            <input type="text" class="form-control pull-right" id="datepicker">
                        </div>
                    </div>
                    <div class="col-xs-4">
                        <label>Last Name:</label>

                        <input type="text" class="form-control" placeholder="" autocomplete="off">
                        <!-- /.Email Address -->
                    </div>
                    <!-- /.form group -->
                    <!-- /.Middle Name -->
                    <div class="col-xs-4">
                        <label>Middle Name:</label>

                        <input type="text" class="form-control" placeholder="" autocomplete="off">
                        <!-- /.Email Address -->
                    </div>
                    <!-- /.First Name -->
                    <div class="col-xs-4">
                        <label>Middle Name:</label>

                        <input type="text" class="form-control" placeholder="" autocomplete="off">
                        <!-- /.Email Address -->
                    </div>
                    <!-- /.Marital status-->
                    <div class="col-xs-3">
                        <label>MaritalStatus:</label>

                        <div class="form-group">
                            <select class="form-control select2" style="width: 100%;">
                                <option selected="selected">Married</option>
                                <option>Single</option>
                                <option>Not Applicable</option>
                            </select>
                            <!-- /.Marital status -->
                        </div>
                    </div>
                    <!-- /.Birth Date -->
                    <div class="col-xs-4">
                        <label>Date:</label>

                        <div class="input-group date">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                            <input type="text" class="form-control pull-right" id="datepicker">
                        </div>
                        <!-- /.input group -->
                    </div>
                    <!-- /.Birth Date -->
                    <!-- /.Age -->
                    <div class="col-xs-2">
                        <label>Age:</label>

                        <input type="number" class="form-control" placeholder="1" autocomplete="off">
                        <!-- /.Age -->
                    </div>
                    <!-- .Sex -->
                    <div class="col-xs-3">
                        <label>Gender:</label>
                        <select class="form-control select2" style="width: 100%;">
                            <option selected="selected">Male</option>
                            <option>Female</option>
                        </select>
                        <!-- /.Sex -->
                    </div>
                    <!-- .Address -->
                    <div class="col-xs-12">
                        <label>Address:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter ..." autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Address -->
                    <!-- .Cell Phone -->
                    <div class="col-xs-6">
                        <label>Cell Phone Number:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="0000-000-000" autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Cell Phone -->
                    <!-- .Home Phone -->
                    <div class="col-xs-6">
                        <label>Home Phone Number:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="0000-000-000" autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Home Phone -->
                    <!-- .Occupation  -->
                    <div class="col-xs-4">
                        <label>Occupation:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="" autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Occupation  -->
                    <!-- .Employer  -->
                    <div class="col-xs-4">
                        <label>Employer:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="" autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Employer  -->
                    <!-- .Employer Contact -->
                    <div class="col-xs-4">
                        <label>EmployerNumber:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="0000-000-000" autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Employer Contact  -->
                    <!-- .Employer Contact -->
                    <div class="col-xs-12">
                        <label>Reason for Visit:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Reason/reffered by..." autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Employer Contact  -->
                </form>

                <!-- /.form group -->
            </div>

            <!-- /.box-body -->
        </div>
    </div>

    <div class="col-md-6">
        <div class="box box-primary">
            <div class="text-center">
                <h3 class="box-title">Insurance Information</h3>
                <small>(Please give your insurance card to the receptionist.)</small>
            </div>
            <div class="box-body">
                            <!-- Date -->
                <div class="form-group">

                    <div class="col-xs-6">
                        <label>Principle Member:</label>
                        <input type="text" class="form-control" placeholder="" autocomplete="off">
                    </div>
                    <!-- /.input group -->
                    <!-- /.form group -->

                    <div class="col-xs-6">
                        <label>Birth Date:</label>
                        <div class="input-group date">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                            <input type="text" class="form-control pull-right" id="datepicker">
                        </div>
                        <!-- /.input group -->
                    </div>
                    <!-- .Address -->
                    <div class="col-xs-12">
                        <label>Address (if different):</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter ..." autocomplete="off">
                        </div>
                    </div>
                    <!-- /.Address -->
                    <!-- .Principle Status -->
                    <div class="col-xs-12">
                        <label>Is the principle a patient here?:</label>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter ..." autocomplete="off">
                        </div>
                    </div>

                            <!-- /.Address -->
                            <!-- Date and time range -->
                            <div class="form-group">
                                <label>Date and time range:</label>

                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-clock-o"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="reservationtime">
                                    <div class="input-group-addon">
                                        <i class="fa fa-clock-o"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="reservationtime">

                                </div>
                                <!-- /.input group -->
                            </div>


                            <!-- /.form group -->

                        </div>
                        <!-- /.box-body -->

                    </div>

                </div>
        </div>
    

